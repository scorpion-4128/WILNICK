<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center"
    android:layout_height="fill_parent"
    android:layout_width="fill_parent"
    android:orientation="vertical">
    
    <androidx.cardview.widget.CardView
        android:layout_height="wrap_content"
        android:layout_margin="10.0dip"
        android:layout_width="fill_parent"
        android:padding="20.0dip"
        app:cardCornerRadius="10.0dip">
        
        <LinearLayout
            android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:orientation="vertical">
            
            <LinearLayout
                android:background="@drawable/butys"
                android:gravity="center"
                android:id="@+id/but1"
                android:layout_height="40dp"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:layout_width="240.0dip"
                android:orientation="vertical">
                
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Update Config"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#ffffffff"
                    android:textSize="17sp"
                    android:textStyle="bold"/>
                
                <TextView
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Update to the config's latest version"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#FFFFFFFF"
                    android:textSize="8sp"/>
                
            
            </LinearLayout>
            
            <LinearLayout
                android:background="@drawable/butys"
                android:gravity="center"
                android:id="@+id/but2"
                android:layout_height="40dp"
                android:layout_marginBottom="5dp"
                android:layout_width="240.0dip"
                android:orientation="vertical">
                
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Update App"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#ffffffff"
                    android:textSize="17sp"
                    android:textStyle="bold"/>
                
                <TextView
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Update to the app's latest version"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#FFFFFFFF"
                    android:textSize="8sp"/>
                
            
            </LinearLayout>
            
            <LinearLayout
                android:background="@drawable/butys"
                android:gravity="center"
                android:id="@+id/but3"
                android:layout_height="40dp"
                android:layout_marginBottom="5dp"
                android:layout_width="240.0dip"
                android:orientation="vertical">
                
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="App Settings"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#ffffffff"
                    android:textSize="17sp"
                    android:textStyle="bold"/>
                
                <TextView
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Manage your app's settings"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#FFFFFFFF"
                    android:textSize="8sp"/>
                
            
            </LinearLayout>
            
            <LinearLayout
                android:background="@drawable/butys"
                android:gravity="center"
                android:id="@+id/but4"
                android:layout_height="40dp"
                android:layout_marginBottom="5dp"
                android:layout_width="240.0dip"
                android:orientation="vertical">
                
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Mobile Browser"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#ffffffff"
                    android:textSize="17sp"
                    android:textStyle="bold"/>
                
                <TextView
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Browse the internet"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#FFFFFFFF"
                    android:textSize="8sp"/>
                
            
            </LinearLayout>
            
            <LinearLayout
                android:background="@drawable/butys"
                android:gravity="center"
                android:id="@+id/but5"
                android:layout_height="40dp"
                android:layout_marginBottom="5dp"
                android:layout_width="240.0dip"
                android:orientation="vertical">
                
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="GTM IP Hunter"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#ffffffff"
                    android:textSize="17sp"
                    android:textStyle="bold"/>
                
                <TextView
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Use to connect to GTM No load No blocking"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#FFFFFFFF"
                    android:textSize="8sp"/>
                
            
            </LinearLayout>
            
            <LinearLayout
                android:background="@drawable/butys"
                android:gravity="center"
                android:id="@+id/but6"
                android:layout_height="40dp"
                android:layout_marginBottom="5dp"
                android:layout_width="240.0dip"
                android:orientation="vertical">
                
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Speed Test"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#ffffffff"
                    android:textSize="17sp"
                    android:textStyle="bold"/>
                
                <TextView
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Determine if the signal is strong or not"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#FFFFFFFF"
                    android:textSize="8sp"/>
                
            
            </LinearLayout>
            
        
        </LinearLayout>
        
    
    </androidx.cardview.widget.CardView>
    

</LinearLayout>